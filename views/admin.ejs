    <div
      class="modal fade"
      id="forslide"
      tabindex="-1"
      role="dialog"
      aria-labelledby="exampleModalLabel"
      aria-hidden="true"
    >
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h4 class="modal-title" id="exampleModalLabel">Modify Contents</h4>
          </div>
          <div class="modal-body">
            <form action="" method="POST">
              <div class="form-group">
                <label for="year">Year</label>
                <input
                  type="text"
                  class="form-control"
                  id="Email-1"
                  name="yr"
                  placeholder="year"
                />
              </div>
              <div class="form-group">
                <label for="sem">Sem</label>
                <input
                  type="text"
                  class="form-control"
                  id="Email-1"
                  name="sems"
                  placeholder="sem"
                />
              </div>
              <div class="form-group">
                <label for="subject">Tag</label>
                <input
                  type="text"
                  class="form-control"
                  id="Email-1"
                  name="subj"
                  placeholder="subject"
                />
              </div>
              <div class="form-group">
                <label for="name">Name</label>
                <input
                  type="text"
                  class="form-control"
                  id="Email-1"
                  name="video"
                  placeholder="video"
                />
              </div>
              <div class="form-group">
                <label for="link">Link</label>
                <input
                  type="url"
                  class="form-control"
                  id="Email-1"
                  name="link"
                  placeholder="link"
                />
              </div>

              <div class="modal-footer">
                <button
                  type="button"
                  class="btn btn-light"
                  data-dismiss="modal"
                >
                  Close
                </button>
                <button class="btn btn-success" type="submit">
                  Add
                </button>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>

